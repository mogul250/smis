{
  "include": [
    "src/**/*.js"
  ],
  "exclude": [
    "node_modules/**",
    "tests/**",
    "coverage/**",
    "**/*.test.js",
    "**/*.spec.js"
  ],
  "reporter": [
    "text",
    "html",
    "lcov"
  ],
  "report-dir": "coverage",
  "cache": true,
  "all": false,
  "check-coverage": false,
  "lines": 0,
  "functions": 0,
  "branches": 0,
  "statements": 0
}
